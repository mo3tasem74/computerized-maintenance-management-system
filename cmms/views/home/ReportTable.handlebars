<!--================start form  Area =================-->
<section class="appointment-area area-padding">
    <div class="container1">

        <div class="appointment-inner">
            <div class="row">
                <div class="col-sm-16 col-lg-10 offset-lg-1">
                    <div class="topnav">

                        <div class="search-container">
                            <form action="/ReportTable" method="post">
                                <input type="text" placeholder="Search by id.." name="search">
                                <button type="submit"><i class="fa fa-search"></i></button>
                            </form>
                        </div>
                    </div>
                    <caption>Report Table</caption>
                    <table id="hakona">
                        <thead>
                        <tr class="table100-head">
                            <th class="column3">Department</th>
                            <th class="column4">Department-Code</th>
                            <th class="column4">Equipment-Name</th>
                            <th class="column5">ID</th>
                            <th class="column4">Manufacturer</th>
                            <th class="column3">Model</th>
                            <th class="column4">Fault-Date</th>
                            <th class="column3">Job-Number</th>
                            <th class="column3">Priority</th>
                            <th class="column4">Technician-Name</th>
                            <th class="column3">Fault-Description</th>
                            <th class="column4">Action-Taken</th>
                            <th class="column3">Solution</th>
                            <th class="column4">End-Date</th>
                        </tr>
                        </thead>
                        <tbody>
                        {{#each reports}}
                            <tr>
                                <td class="column3">{{department}}</td>
                                <td class="column4">{{de_code}}</td>
                                <td class="column4">{{equip_name}}</td>
                                <td class="column5">{{report_id}}</td>
                                <td class="column4">{{manufacturer}}</td>
                                <td class="column3">{{model}}</td>
                                <td class="column4">{{fault_date}}</td>
                                <td class="column3">{{job_no}}</td>
                                <td class="column3">{{priority}}</td>
                                <td class="column4">{{tech_name}}</td>
                                <td class="column3">{{fault_description}}</td>
                                <td class="column4">{{action_taken}}</td>
                                <td class="column3"><a href="/solved?id={{job_no}}" id="{{job_no}}">{{solved}}</a></td>
                                <td class="column4">{{end_date}}</td>


                            </tr>
                        {{/each}}
                        {{#each rep}}
                            <tr>
                                <td class="column3">{{department}}</td>
                                <td class="column4">{{de_code}}</td>
                                <td class="column4">{{equip_name}}</td>
                                <td class="column5">{{report_id}}</td>
                                <td class="column4">{{manufacturer}}</td>
                                <td class="column3">{{model}}</td>
                                <td class="column4">{{fault_date}}</td>
                                <td class="column3">{{job_no}}</td>
                                <td class="column3">{{priority}}</td>
                                <td class="column4">{{tech_name}}</td>
                                <td class="column3">{{fault_description}}</td>
                                <td class="column4">{{action_taken}}</td>
                                <td class="column3"><a href="/solved?id={{job_no}}" id="{{job_no}}">{{solved}}</a></td>
                                <td class="column4">{{end_date}}</td>


                            </tr>
                        {{/each}}
                        </tbody>

                    </table>
                </div>


            </div>

        </div>

</section>
<!-- ================ form Area ends ================= -->
