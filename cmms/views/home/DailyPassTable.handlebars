<!--================start form  Area =================-->
<section class="appointment-area area-padding">
    <div class="container1">

        <div class="appointment-inner">
            <div class="row">
                <div class="col-sm-16 col-lg-10 offset-lg-1">
                    <div class="topnav">

                        <div class="search-container">
                            <form action="/DailyPassTable" method="post">
                                <input type="text" placeholder="Search by id .." name="search">
                                <button type="submit"><i class="fa fa-search"></i></button>
                            </form>
                        </div>
                    </div>
                    <caption>DailyPass Table</caption>
                    <table id="hakona">
                        <thead>
                        <tr class="table100-head">
                            <th class="column4">Department</th>
                            <th class="column4">Department-Code</th>
                            <th class="column5">Equipment-Name</th>
                            <th class="column5">ID</th>
                            <th class="column4">Technician-Name</th>
                            <th class="column3">Physical-Condition</th>
                            <th class="column3">Batteries-Condition</th>
                            <th class="column3">Cables&Ports-Condition</th>
                            <th class="column3">Self-Test</th>
                            <th class="column4">Comment</th>
                            <th class="column4">Inspection-Date</th>
                            <th class="column3">Inspection-Number</th>


                        </tr>


                        </thead>

                        <tbody>
                        {{#each daily_inspection}}
                            <tr>
                                <td class="column4">{{department}}</td>
                                <td class="column4">{{d_code}}</td>
                                <td class="column5">{{nomenclature}}</td>
                                <td class="column5">{{dinspect_id}}</td>
                                <td class="column4">{{tech_name}}</td>
                                <td class="column3">{{physical_condition}}</td>
                                <td class="column3">{{batteries}}</td>
                                <td class="column3">{{cables_port}}</td>
                                <td class="column3">{{self_test}}</td>
                                <td class="column4">{{comment}}</td>
                                <td class="column4">{{inspection_date}}</td>
                                <td class="column3">{{insp_no}}</td>


                            </tr>
                        {{/each}}
                        </tbody>

                    </table>
                </div>


            </div>

        </div>

</section>
<!-- ================ form Area ends ================= -->
